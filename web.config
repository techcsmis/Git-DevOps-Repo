<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.web>
<!--	<identity impersonate="true" /> -->
        <identity impersonate="false" />
    </system.web>
    <system.webServer>
        <directoryBrowse enabled="true" />
        <handlers>
            <remove name="CGI-exe" />
            <add name="CGI-exe" 
                 path="*.exe" 
                 verb="*" 
                 modules="CgiModule" 
                 scriptProcessor="C:\Python\Python379\python.exe" 
                 resourceType="File" 
                 requireAccess="Execute" 
                 allowPathInfo="true" />
        </handlers>
    </system.webServer>
</configuration>
